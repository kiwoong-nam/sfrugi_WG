#!/bin/bash

zcat /home/knam/work/sfrugi_WG/GENE_ANNOTATIONS/seq/assembly/FC_88.fa.gz > /home/knam/work/sfrugi_WG/GENE_ANNOTATIONS/Final/intact/cds/temp.fa

/home/knam/save/programs/gffread/gffread /home/knam/work/sfrugi_WG/GENE_ANNOTATIONS/Final/intact/gff/FC_88.gff -g /home/knam/work/sfrugi_WG/GENE_ANNOTATIONS/Final/intact/cds/temp.fa -x /home/knam/work/sfrugi_WG/GENE_ANNOTATIONS/Final/intact/cds/FC_88.cds.fa

rm /home/knam/work/sfrugi_WG/GENE_ANNOTATIONS/Final/intact/cds/temp.fa*

